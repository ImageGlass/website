@{
  var _page = ViewData[PageInfo.Page]?.ToString() ?? string.Empty;
}
<!DOCTYPE html>
<html lang="en" data-page="@_page">

<head>
  @await Html.PartialAsync("~/Views/_Partials/_Head.cshtml")
</head>

<body>
  <div id="app">
    <header id="appHeader">
      <nav class="navbar container">
        <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">ImageGlass</a>

        <input type="checkbox" id="chkMobileMenu" role="none">
        <label for="chkMobileMenu">
          <span class="bar1"></span>
          <span class="bar2"></span>
          <span class="bar3"></span>
        </label>

        <ul class="navbar-nav">
          <li class="nav-item @(_page.Contains("news") ? "nav-item--active" : "")">
            <a asp-area="" asp-controller="News" asp-action="AllNewsListingPage">News</a>
          </li>

          <li class="nav-item nav-submenu @(_page.Contains("download") ? "nav-item--active" : "")">
            <a asp-area="" asp-controller="Release" asp-action="Download">Download</a>

            <ul>
              <li class="nav-item @(_page.Contains("release") ? "nav-item--active" : "")">
                <a asp-area="" asp-controller="Release" asp-action="AllReleasesListingPage">All releases</a>
              </li>
              <li class="nav-item @(_page.Contains("theme") ? "nav-item--active" : "")">
                <a asp-area="" asp-controller="Theme" asp-action="ThemePackListingPage">Theme packs</a>
              </li>
              <li class="nav-item @(_page.Contains("icon") ? "nav-item--active" : "")">
                <a asp-area="" asp-controller="ExtensionIcon" asp-action="ExtensionIconListingPage">Extension icon
                  packs</a>
              </li>
              <li class="nav-item @(_page.Contains("language") ? "nav-item--active" : "")">
                <a asp-area="" asp-controller="Language" asp-action="LanguageListing">Language packs</a>
              </li>
            </ul>
          </li>

          <li class="nav-item nav-submenu @(_page.Contains("support") ? "nav-item--active" : "")">
            <a asp-area="" asp-controller="Support" asp-action="Index">Support</a>
            <ul>
              <li class="nav-item @(_page.Contains("faqs") ? "nav-item--active" : "")">
                <a asp-area="" asp-controller="Support" asp-action="FAQsPage">FAQs</a>
              </li>
              <li class="nav-item @(_page.Contains("privacy") ? "nav-item--active" : "")">
                <a asp-area="" asp-controller="Support" asp-action="PrivacyPage">Privacy policy</a>
              </li>
              <li class="nav-item @(_page.Contains("license") ? "nav-item--active" : "")">
                <a asp-area="" asp-controller="Support" asp-action="LicensePage">End-user license agreement</a>
              </li>
            </ul>
          </li>

          <li class="nav-item @(_page.Contains("docs") ? "nav-item--active" : "")">
            <a asp-area="" asp-controller="Docs" asp-action="Index">Docs</a>
          </li>

          <li class="nav-item @(_page.Contains("about") ? "nav-item--active" : "")">
            <a asp-area="" asp-controller="About" asp-action="Index">About</a>
          </li>
        </ul>

      </nav>
    </header>


    <main id="appBody" role="main">
      @RenderBody()
    </main>


