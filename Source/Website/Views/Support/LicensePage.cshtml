@model string

<div class="container">
  <div class="row gx-5">
    <div class="col-12 col-lg-8 my-5 aniFadeIn">
      @await Html.PartialAsync("_MarkdownBody", Model)

      <section class="mt-5">
        <h2 class="mb-4">ImageGlass registration form</h2>

        <div class="embed embed-1by1">
          <iframe style="background: #fff; border-radius: 5px;"
            src="https://docs.google.com/forms/d/e/1FAIpQLSdL3CHnJa5zd50TdW1xnL6QmAgS8Ovi5UppAKXseAbnFiR-jg/viewform?embedded=true"
            width="700" height="1000" frameborder="0" marginheight="0" marginwidth="0">
            Loading…
          </iframe>
        </div>
      </section>
    </div>

    <div class="col-12 col-lg-4 my-5 aniSlideUp ani-delay-25">
      <div class="sidebar">
        <div class="sidebar-item">
          @await Html.PartialAsync("_Ads")
        </div>
      </div>
    </div>
  </div>
</div>
